    </div>
    <div class="sm.hidden absolute z-2 top-0 left-0 pointer-events-none w-100vw sm.w-320 h-100vh overflow-hidden" style="mix-blend-mode: overlay">
      <div class="glass-keylight is-docs">
        <div class="glass-keylight-outer">
          <div class="glass-keylight-inner">
          </div>
        </div>
      </div>
    </div>
  </aside>

  <div class="hidden sm.block fixed z-2 top-0 left-0 pointer-events-none w-320 h-100vh overflow-hidden" style="mix-blend-mode: overlay">
    <div class="glass-keylight is-docs">
      <div class="glass-keylight-outer">
        <div class="glass-keylight-inner">
        </div>
      </div>
    </div>
  </div>

  <div class="flex-1 gap-48 sm.border-l-2 border-heading-accent color-white">
    <ul class="explorer-grid" :class="explorerSettings.gridSize">


      {# <template x-for="category in filteredIcons" :key="category.name">
        
        <div>
          <h2 class="color-white" x-text="category.name"></h2>
          <template x-for="icon in category.icons" :key="icon.id">
            <img :src="icon.url" :alt="icon.name" :data-keywords="icon.keywords.join(' ')">
            <div class="color-white" x-text="icon.name">

            </div>
            ICON HEREge
          </template>
        </div>
      </template> #}
      {%- for category in icons.categories -%}
        {%- for icon in category.icons -%}
          <li class="grid-icon" alt="{{ icon.name }}" data-keywords="{{ icon.keywords }}" x-show="iconSettings.styles.line">
            <button>
              <img src="/assets/icons/line/{{ icon.url }}" onload="SVGInject(this)">
            </button>
          </li>
          <li class="grid-icon" alt="{{ icon.name }}" data-keywords="{{ icon.keywords }}" x-show="iconSettings.styles.solid">
            <button>
              <img src="/assets/icons/solid/{{ icon.url }}" onload="SVGInject(this)">
            </button>
          </li>
        {%- endfor -%}
      {%- endfor -%}
    </ul>
  </div>


</div>