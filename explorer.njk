                <button class="flex align-items-center justify-content-between w-100p px-12 py-10 text-sm semibold radius-md transition-200" :class="iconSettings.styles.duoLine ? 'color-white bg-white bg-opacity-8' : 'color-pink-400 hover.color-pink-300 bg-white bg-opacity-0 hover.bg-opacity-4'" @click="iconSettings.styles.duoLine = !iconSettings.styles.duoLine">
